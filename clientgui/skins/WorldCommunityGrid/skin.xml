<?xml version="1.0" encoding="UTF-8"?>
<clientskin>
        <simple>
		<images>
			<background>
				<imgsrc>graphic/wcg_simplegui_bg.bmp</imgsrc>
				<bgcol>195:230:250</bgcol>
			</background>
			<dlgpreferences>
				<imgsrc>graphic/dlgPreferences.bmp</imgsrc>
			</dlgpreferences>
			<dlgmessages>
				<imgsrc>graphic/dlgMessages.bmp</imgsrc>
			</dlgmessages>
			<gauge>
				<fgcol>153:204:255</fgcol>
				<bgcol>204:255:204</bgcool>
			</gauge>
			<buttons>
				<preferences>
					<imgsrc>graphic/btnPreferences.bmp</imgsrc>
				</preferences>
				<addproj>
					<imgsrc>graphic/btnAddProject.bmp</imgsrc>
				</addproj>
				<advancedview>
					<imgsrc>graphic/btnAdvancedView.bmp</imgsrc>
				</advancedview>
				<play>
					<imgsrc>graphic/btnResume.bmp</imgsrc>
				</play>
				<pause>
					<imgsrc>graphic/btnPause.bmp</imgsrc>
				</pause>
				<messages>
					<imgsrc>graphic/btnMessages.bmp</imgsrc>
				</messages>
				<open>
					<imgsrc>graphic/btnOpen.bmp</imgsrc>
				</open>
				<save>
					<imgsrc>graphic/btnSave.bmp</imgsrc>
				</save> 
				<cancel>
					<imgsrc>graphic/btnCancel.bmp</imgsrc>
				</cancel>
				<leftArr>
					<imgsrc>graphic/arwLeft.png</imgsrc>
					<imgsrcclick>graphic/arwLeftClick.png</imgsrcclick>
				</leftArr>
				<rightArr>
					<imgsrc>graphic/arwRight.png</imgsrc>
					<imgsrcclick>graphic/arwRightClick.png</imgsrcclick>
				</rightArr>
				<expand>
					<imgsrc>graphic/btnExpand.png</imgsrc>
					<imgsrcclick>graphic/btnExpandClick.png</imgsrcclick>
				</expand>
				<collapse>
					<imgsrc>graphic/btnCollapse.png</imgsrc>
					<imgsrcclick>graphic/btnCollapseClick.png</imgsrcclick>
				</collapse>
				<showgraphics>
					<imgsrc>graphic/btnShowGraphic.png</imgsrc>
					<imgsrcclick>graphic/btnShowGraphicClick.png</imgsrcclick>
				</showgraphics>
		        </buttons>
			<icons>
				<working>
					<imgsrc>graphic/working.bmp</imgsrc>
				</working> 
				<sleeping>
					<imgsrc>graphic/sleeping.bmp</imgsrc>
				</sleeping>
				<workingWkUnit>
					<imgsrc>graphic/ico_workWU.png</imgsrc>
					<frcol>255:255:255</frcol>
					<tocol>96:206:234</tocol>
					<brdcol>73:193:223</brdcol>
				</workingWkUnit>
				<sleepingWkUnit>
					<imgsrc>graphic/ico_sleepWU.png</imgsrc>
					<frcol>255:255:255</frcol>
					<tocol>155:229:248</tocol>
					<brdcol>216:234:245</brdcol>
				</sleepingWkUnit>
				<defaultProjIcon>
					<imgsrc>graphic/default_proj_icon.png</imgsrc>
				</defaultProjIcon>
				<defaultStatIcon>
					<imgsrc>graphic/default_stat_icon.png</imgsrc>
				</defaultStatIcon>
			</icons>
			<animation>
				<background>graphic/project_image.bmp</background>
				<animation>graphic/molecule.gif</animation>
			</animation>
		</images>
	</simple>
	<advanced>  <!-- this section can be omitted in which case it will use the BOINC defaults (shown below) -->
		<application_name>BOINC Manager</application_name>  <!-- name displayed in application title bar -->
		<application_icon>boinc_xpm</application_icon> <!-- where is this used? -->
		<application_logo>boincsm_xpm</application_logo> <!-- logo displayed in system tray -->
		<company_name>Space Sciences Laboratory, U.C. Berkeley</company_name> <!-- where is this used? -->
		<company_website>http://boinc.berkeley.edu/</company_website> <!-- where is this used? -->
		<project_name>BOINC</project_name>  <!-- where is this used -->
		<open_tab>0</open_tab>  <!-- this setting makes the BOINC manager automatically open to a particular tab on the advanced view, overriding a users last displayed view.  0 means use users last open tab -->
	</advanced>
        <wizards>  <!-- this section can be omitted in which case it will use the BOINC defaults (shown below) -->
        <project_attach>
            <title>Attach to Project</title> <!-- title displayed on the attach to project wizard -->
            <logo>res/attachprojectwizard.xpm</logo>  <!-- logo displayed on the attach to project wizard -->
        </project_attach>
        <account_manager_attach>
            <title>Account Manager</title>  <!-- title displayed on the attach to account manager wizard -->
            <logo>res/attachprojectwizard.xpm</logo>  <!-- logo displayed on the attach to account manager wizard -->
        </account_manager_attach>
    </wizards>
</clientskin>